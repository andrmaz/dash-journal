    "react-dom": "^17.0.2",
    "react-feather": "^2.0.10",
    "recharts": "^2.5.0",
    "remix-typedjson": "^0.1.7",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.0.2",
    "@faker-js/faker": "^6.3.1",
