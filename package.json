    "recharts": "^2.6.2",
    "remix-typedjson": "^0.1.7",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@faker-js/faker": "^6.3.1",
